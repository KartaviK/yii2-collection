{
    "name": "kartavik/yii2-collection",
    "description": "Active Record Collection implementation for the Yii framework",
    "keywords": [
        "yii2",
        "collection",
        "active-record"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/KartaviK/yii2-collection/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/KartaviK/yii2-collection"
    },
    "authors": [
        {
            "name": "Carsten Brandt",
            "email": "mail@cebe.cc"
        },
        {
            "name": "Roman Varkuta",
            "email": "roman.varkuta@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.0",
        "ext-json": "*",
        "yiisoft/yii2": "^2.0.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.4",
        "vlucas/phpdotenv": "^2.5",
        "yiisoft/yii2-phpunit": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Kartavik\\Yii\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kartavik\\Yii\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Horat1us/yii2-phpunit.git"
        }
    ],
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1"
        }
    },
    "scripts": {
        "lint": "phpcs --standard=PSR2 ./src ./tests",
        "phpcbf": "phpcbf --standard=PSR2 ./src ./tests",
        "cover": "phpunit --coverage-clover=coverage.xml",
        "test": "phpunit"
    }
}
